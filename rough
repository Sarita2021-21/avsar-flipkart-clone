     <script>
        let slideIndex=0;
        showSlides();

        function showSlides(){
            let i;
            let slides=document.getElementsByClassName("carousel-div");
            for(i=0;i<slide.length;i++){
                slides[i].style.display="none";
            }
            slideIndex++;
        }
    </script>


